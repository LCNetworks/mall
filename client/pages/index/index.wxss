.body{
  height: 90%;
}

.swiper-img{
  width:100%;
  text-align: center
}

.sbtn{
  float:left;
  display:-webkit-flex;
  flex-direction:column;
  flex-wrap:wrap;
  width:25%;
  text-align:center;
  margin:10rpx 0;
}
.sbtn-img{
  margin:0 auto;
  width:100rpx;
  height:100rpx;
}

.recommImgs{
  width: 95%;
}

.contain-item-left{
  float: left;
  width:50%;
  margin: 0 auto;
  text-align: center;
  margin-top: 10px;
}
.contain-item-right{
  float:right;
  width:48%;
  margin: 0 auto;
  text-align: center;
  margin-top: 10px;
}

.items-title{
  margin-left:2%;
  font-size:12px;
  vertical-align: middle
}

.item-img{
  width:90%;
  height:100px;
}
.item-text{
  display: inline-block;
  vertical-align: middle;
  font-size:13px;
}
.item-text-price{
  color: red
}
.viewAll-btn{
  text-align: center;
  background-color: white;
  font-size: 15px;
  position:relative;
  top:10px;
}
.foot{
  margin-bottom: 25px;
  text-align:center;
  font-size: 12px;
  color: gray;
  position:relative;
  top:10px;
}
